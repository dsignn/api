{
    "name": "dsignn/api",
    "description": "Rest full Api of dsignn application",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "https://github.com/dsignn/api",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonino Visalli",
            "email": "antonino.visalli@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-json": "*",
        "ext-mongodb": "*",
        "alcaeus/mongo-php-adapter": "^1.1.11",
        "aws/aws-sdk-php": "^3.1.0",
        "defuse/php-encryption": "^2.2.1.",
        "endroid/qr-code": "~3.9.0",
        "guzzlehttp/guzzle": "~7.8.0",
        "laminas/laminas-crypt": "3.11.0",
        "laminas/laminas-eventmanager": "3.12.0",
        "laminas/laminas-hydrator": "^4.15.0",
        "laminas/laminas-inputfilter": "^2.28.0",
        "laminas/laminas-permissions-acl": "^2.16.0",
        "league/oauth2-server": "^8.0.0",
        "mongodb/mongodb": "^1.5",
        "monolog/monolog": "^1.24",
        "notihnio/php-request-parser": "1.0.0",
        "php-di/php-di": "^6.0",
        "php-di/slim-bridge": "^3.0",
        "php-ffmpeg/php-ffmpeg": "^1.1.0",
        "phpmailer/phpmailer": "~6.1.5",
        "rikudou/array-merge-recursive": "^1.0",
        "sendgrid/sendgrid": "7.9.2",
        "sendinblue/api-v3-sdk": "8.4.2",
        "slim/psr7": "^1.5.1",
        "slim/slim": "^4.12",
        "slim/twig-view": "^2.5.1",
        "symfony/console": "^5.0.2",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.31"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
