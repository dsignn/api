{
    "name": "dsignn/api",
    "description": "Rest full Api of dsignn application",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "https://github.com/dsignn/api",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonino Visalli",
            "email": "antonino.visalli@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "alcaeus/mongo-php-adapter": "^1.1.11",
        "aws/aws-sdk-php": "^3.1.0",
        "bacon/bacon-qr-code": "~2.0.0",
        "defuse/php-encryption": "^2.2.1.",
        "guzzlehttp/guzzle": "~7.0.0",
        "laminas/laminas-eventmanager": "3.2.1",
        "laminas/laminas-hydrator": "^3.0.2",
        "laminas/laminas-inputfilter": "^2.10.1",
        "laminas/laminas-permissions-acl": "^2.7.1",
        "league/oauth2-server": "^8.0.0",
        "mongodb/mongodb": "^1.5",
        "monolog/monolog": "^1.24",
        "notihnio/php-request-parser": "1.0.0",
        "php-di/php-di": "^6.0",
        "php-di/slim-bridge": "^3.0",
        "php-ffmpeg/php-ffmpeg": "^0.14.0",
        "phpmailer/phpmailer": "~6.1.5",
        "slim/psr7": "^0.5",
        "slim/slim": "^4.4",
        "symfony/console": "^5.0.2",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
